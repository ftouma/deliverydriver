<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content">
  <TextView
    android:id="@+id/runcompletephone"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentTop="true"
    android:layout_alignParentLeft="true">
  </TextView>
  <TextView
    android:id="@+id/runtip"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentTop="true"
    android:layout_alignParentRight="true">    
  </TextView>
  <TextView
    android:id="@+id/runtiplabel"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentTop="true"
    android:layout_toLeftOf="@+id/runtip"
    android:text="Tip: ">
  </TextView>
  <TextView
    android:id="@+id/runcompleteaddress"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/runcompletephone"
    android:layout_alignParentLeft="true">
  </TextView>
  <TextView
    android:id="@+id/runtime"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/runtip"
    android:layout_alignParentRight="true">
  </TextView>
</RelativeLayout>
